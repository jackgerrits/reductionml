{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Configuration",
  "type": "object",
  "required": [
    "entryReduction",
    "globalConfig"
  ],
  "properties": {
    "$schema": {
      "pattern": ".*"
    },
    "entryReduction": {
      "$ref": "#/definitions/any_reduction_config"
    },
    "globalConfig": {
      "$ref": "#/definitions/GlobalConfig"
    }
  },
  "additionalProperties": false,
  "definitions": {
    "Binary": {
      "properties": {
        "config": {
          "title": "BinaryReductionConfig",
          "type": "object",
          "properties": {
            "regressor": {
              "default": {
                "config": {
                  "alpha": 4.0,
                  "beta": 1.0,
                  "l1Lambda": 0.0,
                  "l2Lambda": 0.0,
                  "linkFunction": "Identity",
                  "lossFunction": {
                    "Squared": {}
                  }
                },
                "typename": "Coin"
              },
              "allOf": [
                {
                  "$ref": "#/definitions/any_reduction_config"
                }
              ]
            }
          },
          "additionalProperties": false
        },
        "typename": {
          "const": "Binary"
        }
      },
      "additionalProperties": false
    },
    "CBType": {
      "type": "string",
      "enum": [
        "ips",
        "mtr"
      ]
    },
    "CbAdf": {
      "properties": {
        "config": {
          "title": "CBAdfConfig",
          "type": "object",
          "properties": {
            "cbType": {
              "default": "mtr",
              "allOf": [
                {
                  "$ref": "#/definitions/CBType"
                }
              ]
            },
            "regressor": {
              "default": {
                "config": {
                  "alpha": 4.0,
                  "beta": 1.0,
                  "l1Lambda": 0.0,
                  "l2Lambda": 0.0,
                  "linkFunction": "Identity",
                  "lossFunction": {
                    "Squared": {}
                  }
                },
                "typename": "Coin"
              },
              "allOf": [
                {
                  "$ref": "#/definitions/any_reduction_config"
                }
              ]
            }
          },
          "additionalProperties": false
        },
        "typename": {
          "const": "CbAdf"
        }
      },
      "additionalProperties": false
    },
    "CbExploreAdfGreedy": {
      "properties": {
        "config": {
          "title": "CBExploreAdfGreedyConfig",
          "type": "object",
          "properties": {
            "cbAdf": {
              "default": {
                "config": {
                  "cbType": "mtr",
                  "regressor": {
                    "config": {
                      "alpha": 4.0,
                      "beta": 1.0,
                      "l1Lambda": 0.0,
                      "l2Lambda": 0.0,
                      "linkFunction": "Identity",
                      "lossFunction": {
                        "Squared": {}
                      }
                    },
                    "typename": "Coin"
                  }
                },
                "typename": "CbAdf"
              },
              "allOf": [
                {
                  "$ref": "#/definitions/any_reduction_config"
                }
              ]
            },
            "epsilon": {
              "default": 0.05000000074505806,
              "type": "number",
              "format": "float"
            }
          },
          "additionalProperties": false
        },
        "typename": {
          "const": "CbExploreAdfGreedy"
        }
      },
      "additionalProperties": false
    },
    "CbExploreAdfSoftmax": {
      "properties": {
        "config": {
          "title": "CBExploreAdfSoftmaxConfig",
          "type": "object",
          "properties": {
            "cbAdf": {
              "default": {
                "config": {
                  "cbType": "mtr",
                  "regressor": {
                    "config": {
                      "alpha": 4.0,
                      "beta": 1.0,
                      "l1Lambda": 0.0,
                      "l2Lambda": 0.0,
                      "linkFunction": "Identity",
                      "lossFunction": {
                        "Squared": {}
                      }
                    },
                    "typename": "Coin"
                  }
                },
                "typename": "CbAdf"
              },
              "allOf": [
                {
                  "$ref": "#/definitions/any_reduction_config"
                }
              ]
            },
            "lambda": {
              "default": 1.0,
              "type": "number",
              "format": "float"
            },
            "uniformEpsilon": {
              "default": 0.0,
              "type": "number",
              "format": "float"
            }
          },
          "additionalProperties": false
        },
        "typename": {
          "const": "CbExploreAdfSoftmax"
        }
      },
      "additionalProperties": false
    },
    "CbExploreAdfSquareCb": {
      "properties": {
        "config": {
          "title": "CBExploreAdfSquareCBConfig",
          "type": "object",
          "properties": {
            "cbAdf": {
              "default": {
                "config": {
                  "cbType": "mtr",
                  "regressor": {
                    "config": {
                      "alpha": 4.0,
                      "beta": 1.0,
                      "l1Lambda": 0.0,
                      "l2Lambda": 0.0,
                      "linkFunction": "Identity",
                      "lossFunction": {
                        "Squared": {}
                      }
                    },
                    "typename": "Coin"
                  }
                },
                "typename": "CbAdf"
              },
              "allOf": [
                {
                  "$ref": "#/definitions/any_reduction_config"
                }
              ]
            },
            "gammaExponent": {
              "default": 0.5,
              "type": "number",
              "format": "float"
            },
            "gammaScale": {
              "default": 10.0,
              "type": "number",
              "format": "float"
            },
            "uniformEpsilon": {
              "default": 0.0,
              "type": "number",
              "format": "float"
            }
          },
          "additionalProperties": false
        },
        "typename": {
          "const": "CbExploreAdfSquareCb"
        }
      },
      "additionalProperties": false
    },
    "Coin": {
      "properties": {
        "config": {
          "title": "CoinRegressorConfig",
          "type": "object",
          "properties": {
            "alpha": {
              "default": 4.0,
              "type": "number",
              "format": "float"
            },
            "beta": {
              "default": 1.0,
              "type": "number",
              "format": "float"
            },
            "l1Lambda": {
              "default": 0.0,
              "type": "number",
              "format": "float"
            },
            "l2Lambda": {
              "default": 0.0,
              "type": "number",
              "format": "float"
            },
            "linkFunction": {
              "default": "Identity",
              "allOf": [
                {
                  "$ref": "#/definitions/LinkFunction"
                }
              ]
            },
            "lossFunction": {
              "default": {
                "Squared": {}
              },
              "allOf": [
                {
                  "$ref": "#/definitions/LossFunction"
                }
              ]
            }
          },
          "additionalProperties": false
        },
        "typename": {
          "const": "Coin"
        }
      },
      "additionalProperties": false
    },
    "Debug": {
      "properties": {
        "config": {
          "title": "DebugConfig",
          "type": "object",
          "properties": {
            "features": {
              "default": false,
              "type": "boolean"
            },
            "id": {
              "default": "",
              "type": "string"
            },
            "indent": {
              "default": 0,
              "type": "integer",
              "format": "uint",
              "minimum": 0.0
            },
            "label": {
              "default": false,
              "type": "boolean"
            },
            "next": {
              "default": {
                "config": {},
                "typename": "Unknown"
              },
              "allOf": [
                {
                  "$ref": "#/definitions/any_reduction_config"
                }
              ]
            },
            "prediction": {
              "default": false,
              "type": "boolean"
            }
          },
          "additionalProperties": false
        },
        "typename": {
          "const": "Debug"
        }
      },
      "additionalProperties": false
    },
    "ElementwiseInteraction": {
      "properties": {
        "config": {
          "title": "ElementwiseInteractionConfig",
          "type": "object",
          "properties": {
            "keepOriginalFeatures": {
              "description": "Default is false.",
              "default": false,
              "type": "boolean"
            },
            "pair": {
              "description": "TODO: document that is it pairs for now. But could conceptually be more. FIXME: this default doesn't really make sense.",
              "default": [
                "Default",
                "Default"
              ],
              "type": "array",
              "items": [
                {
                  "$ref": "#/definitions/NamespaceDef"
                },
                {
                  "$ref": "#/definitions/NamespaceDef"
                }
              ],
              "maxItems": 2,
              "minItems": 2
            },
            "regressor": {
              "default": {
                "config": {
                  "alpha": 4.0,
                  "beta": 1.0,
                  "l1Lambda": 0.0,
                  "l2Lambda": 0.0,
                  "linkFunction": "Identity",
                  "lossFunction": {
                    "Squared": {}
                  }
                },
                "typename": "Coin"
              },
              "allOf": [
                {
                  "$ref": "#/definitions/any_reduction_config"
                }
              ]
            }
          },
          "additionalProperties": false
        },
        "typename": {
          "const": "ElementwiseInteraction"
        }
      },
      "additionalProperties": false
    },
    "GlobalConfig": {
      "type": "object",
      "properties": {
        "constantFeatureEnabled": {
          "default": true,
          "type": "boolean"
        },
        "hashSeed": {
          "default": 0,
          "type": "integer",
          "format": "uint32",
          "minimum": 0.0
        },
        "interactions": {
          "default": [],
          "type": "array",
          "items": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/NamespaceDef"
            }
          }
        },
        "numBits": {
          "default": 18,
          "type": "integer",
          "format": "uint8",
          "minimum": 0.0
        }
      },
      "additionalProperties": false
    },
    "LinkFunction": {
      "type": "string",
      "enum": [
        "Identity",
        "Logistic"
      ]
    },
    "LogisticLoss": {
      "type": "object",
      "properties": {
        "max_label": {
          "default": 1.0,
          "type": "number",
          "format": "float"
        },
        "min_label": {
          "default": -1.0,
          "type": "number",
          "format": "float"
        }
      }
    },
    "LossFunction": {
      "oneOf": [
        {
          "type": "object",
          "required": [
            "Squared"
          ],
          "properties": {
            "Squared": {
              "$ref": "#/definitions/SquaredLoss"
            }
          },
          "additionalProperties": false
        },
        {
          "type": "object",
          "required": [
            "Logistic"
          ],
          "properties": {
            "Logistic": {
              "$ref": "#/definitions/LogisticLoss"
            }
          },
          "additionalProperties": false
        }
      ]
    },
    "NamespaceDef": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "Default"
          ]
        },
        {
          "type": "object",
          "required": [
            "Name"
          ],
          "properties": {
            "Name": {
              "type": "string"
            }
          },
          "additionalProperties": false
        }
      ]
    },
    "SquaredLoss": {
      "type": "object"
    },
    "any_reduction_config": {
      "oneOf": [
        {
          "$ref": "#/definitions/Binary"
        },
        {
          "$ref": "#/definitions/CbAdf"
        },
        {
          "$ref": "#/definitions/CbExploreAdfGreedy"
        },
        {
          "$ref": "#/definitions/CbExploreAdfSoftmax"
        },
        {
          "$ref": "#/definitions/CbExploreAdfSquareCb"
        },
        {
          "$ref": "#/definitions/Coin"
        },
        {
          "$ref": "#/definitions/Debug"
        },
        {
          "$ref": "#/definitions/ElementwiseInteraction"
        }
      ]
    }
  }
}
